  type: "ReLU"
  bottom: "main-conv2"
  top: "main-conv2"
}
layer {
  name: "out-seg"
  type: "Convolution"
  bottom: "main-conv2"
  top: "out-seg"
  convolution_param {
    num_output: 13
    pad: 1
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "msra"
    }
  }
}
layer {
  name: "prob"
  type: "Softmax"
  bottom: "out-seg"
  top: "prob"
}
layer {
  name: "silence"
  type: "Silence"
  bottom: "prob"
}
layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "out-seg"
  bottom: "seg_gt"
  top: "accuracy"
  top: "per_class_accuracy"
  accuracy_param {
    ignore_label: 13
  }
}
layer {
  name: "loss-seg"
  type: "SoftmaxWithLoss"
  bottom: "out-seg"
  bottom: "seg_gt"
  top: "loss-seg"
  loss_param {
    ignore_label: 13
    class_weighting: 0.461722187631
    class_weighting: 0.657963212058
    class_weighting: 8.79448495816
    class_weighting: 0.309803065773
    class_weighting: 0.402088859194
    class_weighting: 0.331462683075
    class_weighting: 3.97027156227
    class_weighting: 1.85295437663
    class_weighting: 0.719337211133
    class_weighting: 2.38249690493
    class_weighting: 0.897501529475
    class_weighting: 1.12892839766
    class_weighting: 2.97937534026
  }
}
